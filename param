  choice choices: ['dev', 'test', 'stg', 'prof'], description: 'choose the appication environmen', name: 'deployEnv'
  choice choices: ['dev', 'test', 'stg', 'prof'], description: 'choose the branch to deploy', name: 'deployname'
}

